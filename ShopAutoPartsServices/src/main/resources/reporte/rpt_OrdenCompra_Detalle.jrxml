	<parameter name="numoc" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT OCD.CHRCODIGOPRODUCTO, P.VCHDESCRIPCION, OCD.NUMCANTIDADPEDIDA,
       OCD.NUMPRECIOUNITARIO, OCD.NUMPRECIOTOTAL, U.VCHABREVIATURA,
       M.VCHABREVIATURA AS MONEDA
FROM ORDENCOMPRADETALLE OCD
LEFT JOIN PRODUCTO P ON P.CHRCODIGOPRODUCTO = OCD.CHRCODIGOPRODUCTO
LEFT JOIN UNIDADMEDIDA U ON U.NUMCODIGOMEDIDA = P.NUMCODIGOMEDIDA
LEFT JOIN ORDENCOMPRA OC ON OC.CHRCODIGOOC = OCD.CHRCODIGOOC
LEFT JOIN MONEDA M ON M.NUMCODIGOMONEDA = OC.NUMCODIGOMONEDA
WHERE TRIM(OC.CHRCODIGOOC) = TRIM($P{numoc})]]>
	</queryString>
