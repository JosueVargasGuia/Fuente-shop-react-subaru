	<parameter name="numguia" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  DISTINCT(GD.CHRCODIGOPRODUCTO),
PKG_BOLETA.REPBOLETA(G.NUMCODIGOOS,GD.CHRCODIGOPRODUCTO) AS BOLETA,
GD.VCHUBICACION,PRO.VCHDESCRIPCION,UM.VCHABREVIATURA,GD.NUMPRECIOVENTA,GD.NUMCANTIDAD,
(GD.NUMPRECIOVENTA*GD.NUMCANTIDAD) AS SUBTOTAL,GD.NUMPORCENTAJEDESCUENTO,GD.NUMDESCUENTO,
(GD.NUMPRECIOVENTA*GD.NUMCANTIDAD)-(GD.NUMPRECIOVENTA*(GD.NUMPORCENTAJEDESCUENTO/100)) AS TOTAL,
GD.NUMPRECIOVENTA,
((GD.NUMPRECIOVENTA*GD.NUMCANTIDAD)-(GD.NUMPRECIOVENTA*(GD.NUMPORCENTAJEDESCUENTO/100)))* PKG_CALCULOS.GET_IGV AS IGV,
       CEIL(((((GD.NUMPRECIOVENTA*GD.NUMCANTIDAD)-(GD.NUMPRECIOVENTA*(GD.NUMPORCENTAJEDESCUENTO/100)))* PKG_CALCULOS.GET_IGV)+
(GD.NUMPRECIOVENTA*GD.NUMCANTIDAD)-(GD.NUMPRECIOVENTA*(GD.NUMPORCENTAJEDESCUENTO/100)))*100)/100 AS TOTALPRECIO
FROM GUIADETALLE GD
LEFT JOIN GUIA G ON GD.CHRCODIGOGUIA = G.CHRCODIGOGUIA
LEFT JOIN FACTURA F ON F.CHRCODIGOCOTIZACION=G.NUMCODIGOOS
--LEFT JOIN GUIA G ON GD.NUMCODIGOOS = G.NUMCODIGOOS
LEFT JOIN PRODUCTO PRO ON GD.CHRCODIGOPRODUCTO = PRO.CHRCODIGOPRODUCTO
LEFT JOIN UNIDADMEDIDA UM ON UM.NUMCODIGOMEDIDA = PRO.NUMCODIGOMEDIDA
WHERE trim(GD.CHRCODIGOGUIA) = trim($P{numguia})
AND PRO.CHRCODIGOPRODUCTO = GD.CHRCODIGOPRODUCTO
GROUP BY GD.CHRCODIGOPRODUCTO,GD.VCHUBICACION,PRO.VCHDESCRIPCION,
UM.VCHABREVIATURA,GD.NUMPRECIOVENTA,GD.NUMCANTIDAD,
GD.NUMPORCENTAJEDESCUENTO,GD.NUMDESCUENTO,G.NUMCODIGOOS,
GD.NUMPRECIOVENTA]]>
	</queryString>
